n=int(input("enter a number"))
for i in range(1,n+1) :
      for j in range(1,i+1) :
            print(j, end='')
      print()
######. rows and columns
r=int(input("enter number of rows"))
c=int(input("enter number of columns"))
x=[]
for i in range(0,r) :
      for j in rnage(0,c):
            val.insert(j,int(input("enter the %d * %d element" %(i,j))))
      x.insert(i,val)
      val=[]
y=[]
for i in range(0,r) :
      for j in range(0,c):
            val.insert(j,int(input("enter the %d * %d element" %(i,j))))
      x.insert(i,val)
      val=[]
sum=[]
for i in range(0,r) :
      for j in range(0,c) :
            val.insert(j,x[i][j]+y[i][j])
      sum.insert(i,val)
      val=[]
print(sum)


