x= [[1,2,3],["a","b","c"]]
for i in x :
      for j in i :
            print(j, end="")
      print()

########## break
x= "hello world. accenture"
for i in x :
      if i == "." :
            break
      print(i, end="")
###### continue
for i in [1,13,56,4,6] :
      if i>10 :
            continue
      print(i)
###### for loop
x= [1,4.2,"accenture"]
for i in x :
      print(i)
####char by char
x= "accenture"
for i in x :
      print(i)
##### range
sum=0
for i in range(0,21) :
      if i%2==0 :
            sum=sum+i
      print(sum)
